{
    "parser":"babel-eslint",
    "env":{
        "es6":true,
        "browser":true,
        "node":true
    },
    "rules":{
        "semi":"off",
        "arrow-parents":"off",
        "comma-dangle":"off",
        "require-jsdoc":"off",
        "no-trailing-spaces":"off",
        "operator-linebreak":"off",
        "arrow-parens":"off",
        "max-len":"off",
        "linebreak-style":"off"
    },
    "extends":["eslint:recommended", "google"]
}